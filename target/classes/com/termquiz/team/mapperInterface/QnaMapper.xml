<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.termquiz.team.mapperInterface.QnaMapper">

	
<select id="selectList" resultType="com.termquiz.team.vo.QnaVO">
select qnaNo, qnaId, qnaTitle,qnaContent,qnaTime from
qna order by qnaNo desc
</select>

<select id="selectOne" resultType="com.termquiz.team.vo.QnaVO">
select * from qna where qnaNo=#{qnaNo}
</select>


	
</mapper>